    "cloudinary": "^2.7.0",
    "dotenv": "^17.2.2",
    "firebase": "^12.1.0",
    "framer-motion": "^12.23.12",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.19.0",
    "mongoose": "^8.18.1",
    "multer": "^2.0.2",
    "next": "15.4.5",
    "node-fetch": "^3.3.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "swiper": "^11.2.10"
